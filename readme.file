    }
    
    .limitedtimeHeadingDiv {
        width: 100%;
        display: flex;
        justify-content: center;
    }
    
    .limitedtimeHeading {
        font-weight: 700;
        font-size: 3.2rem;
        color: #2f3337;
        line-height: 1.2;
        margin: 0 0 3.2rem;
    }
    
    .limitedtimeDiv {
        width: 100%;
    }
    
    .universalSpacing {
        padding: 3.2rem 0;
    }
    
    .universalSpacingX {
        padding: 2.4rem;
    }
    
    .offerContainer,
    .categoryContainer {
        display: grid;
        grid-template-columns: repeat(6, 1fr);
        width: 100%;
        row-gap: auto;
    }
    
    .offerCard {
        width: 18.5rem;
    }
    
    .categoryCard {
        width: 18.2rem;
    }
    
    .offerCardLink,
