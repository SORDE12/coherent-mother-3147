    /* slider  */
    
    .mainSlider {
        width: 100%;
        position: relative;
        overflow: hidden;
    }
    
    .slider {
        grid-gap: 0.8rem;
        gap: 1.9rem;
        padding: 0 0.8rem;
        margin: 0 -0.8rem;
        display: flex;
        left: 0;
        transition: left 0.3s ease-out;
        position: relative;
    }
    
    .SliderCardp {
        border: 1 solid #fff;
        box-sizing: content-box;
        min-width: 29rem;
        max-width: 29rem;
    }
    
    .SliderCard {
        flex-shrink: 0;
        width: 100%;
    }
    
    .SliderCardLink,
    .SliderCardLink:active,
    .SliderCardLink:hover {
        text-decoration: none;
        color: inherit;
    }
    
    .SliderCardLink img {
        width: 100%;
    }
    
    .sliderCardDiv {
        height: 100%;
        width: 100%;
        padding: 1.6rem 2.4rem;
        display: -webkit-flex;
        display: flex;
        flex-direction: column;
        flex-grow: 1;
        color: #000;
        background-color: #f5f5f5;
    }
    
    .sliderCardHeading {
        color: #2f3337;
        font-weight: 700;
        font-size: 2.4rem;
        line-height: 2.9rem;
        margin-bottom: 0.8rem;
    }
    
    .sliderCardTitle {
        color: #2f3337;
        font-size: 1.6rem;
        line-height: 1.9rem;
    }
    
    .mainSlider .btn {
        position: absolute;
        top: 40%;
        text-decoration: none;
        padding-left: 0.1rem;
        padding-right: 0.2rem;
        margin: -0.1rem;
        background-color: white;
        border: wheat;
        border-radius: 0.3rem;
        cursor: pointer;
    }
    
    #nextwatch {
        right: 0;
    }
    
    #prevwatch:hover,
    #nextwatch:hover {
        color: rgb(192, 192, 192);
    }
    /* end slider */
    
    .styleCardDiv {
        display: flex;
        overflow: hidden;
        gap: 1.9rem;
        left: 0;
    }
    
    .styleCard {
        max-width: 38.6rem;
    }
    
    .seeAllButtonDiv {
        width: 100%;
        display: flex;
        justify-content: center;
    }
    
    .seeAllButtonLink,
    .seeAllButtonLink:active,
    .seeAllButtonLink:hover {
        text-decoration: none;
        color: inherit;
    }
    
    .seeAllButton {
        margin-top: 1.6rem;
        width: 15rem;
        height: 4.4rem;
        border-radius: 0.4rem;
        font-size: 1.6rem;
        display: flex;
        align-items: center;
        justify-content: center;
    }
    
    .trendingSearchDiv {
        display: flex;
        flex-wrap: wrap;
        gap: 1.9rem;
        left: 0;
    }
    
    .trendingSearchDivLink {
        width: 28.6rem;
    }
    
    .trendingSearch {
        height: 100%;
    }
    
    .trendingSearchText {
        width: 100%;
        height: 100%;
        padding: 1.6rem;
        font-size: 1.6rem;
        font-weight: 700;
        display: flex;
        align-items: center;
        justify-content: center;
        text-align: center;
    }
    
    @media (max-width: 1024px) {
        html {
            font-size: 55%;
        }
        .offerContainer,
        .categoryContainer {
            display: grid;
            grid-template-columns: repeat(5, 1fr);
            width: 100%;
            row-gap: 1rem;
            justify-content: space-between;
